{
    "manifest_version": 3,
    "name": "colorpicky",
    "version": "1.0.0",
    "description": "Pick any color",
    "icons": {
        "16": "icons/16_pick.png",
        "32": "icons/48_pick.png",
        "48": "icons/48_pick.png",
        "128": "icons/128_pick.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "storage"
    ]
}