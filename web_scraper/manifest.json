{
    "manifest_version": 3,
    "name": "scraper",
    "version": "1.0.0",
    "description": "Simple web scraper",
    "action": {
        "default_popup": "popout.html"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "storage",
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ],
            "run_at": "document_end"
        }
    ]
}